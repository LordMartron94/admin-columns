{
    "name": "admincolumns",
    "version": "1.0.0",
    "description": "Sass and JS for Admin Columns",
    "private": true,
    "devDependencies": {
        "@types/jquery": "^3.5.5",
        "@types/node": "^14.14.37",
        "axios": "^0.21.0",
        "jquery": "^3.5.1",
        "js-cookie": "^2.2.1",
        "nanobus": "4.4.0",
        "nps": "^5.9.12",
        "nps-utils": "^1.7.0",
        "sass": "^1.32.12",
        "svg-symbols": "1.0.4",
        "svgstore-cli": "1.3.1",
        "ts-loader": "^7.0.5",
        "typescript": "^4.2",
        "webfont": "8.1.4",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11",
        "wp-pot": "latest"
    },
    "scripts": {
        "ac:build:production": "nps build",
        "ac:build:development": "nps build.development",
        "ac:languages": "nps languages",
        "webfont": "node ./scripts/webfont.js",
        "ac:svgsymbols": "svgstore ./svg-symbols/*.svg -o ../assets/images/symbols.svg"
    },
    "webfont": {
        "formats": [
            "woff2",
            "woff"
        ],
        "template": "./webfont/template.scss.njk",
        "fontName": "cpac_icons",
        "fontHeight": "1000",
        "templateFontPath": "../fonts/",
        "templateClassName": "cpacicon",
        "normalize": true
    }
}
